<div>
   
   
       @{
        Players obj=(Players)ViewData["display"];
       <p> <h3>@obj.Id</h3>
        <h3>@obj.Name</h3>
        <h3>@obj.City</h3>
        </p>
       }

       <form action="/Player/Update/@obj.Id">
        <button type="submit" class="btn btn-default">Update</button>
       </form>
